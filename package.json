{
  "name": "@nithin93/sri-js",
  "version": "1.1.1",
  "description": "A lightweight library to enforce Subresource Integrity (SRI) for dynamically loaded scripts in the browser and to update script tags in HTML using Cheerio.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:client",
    "build:lib": "tsc",
    "build:client": "node scripts/build-client.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "sri",
    "subresource-integrity",
    "security",
    "integrity",
    "cheerio",
    "html",
    "script",
    "browser",
    "typescript"
  ],
  "author": "Nithin Murali",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "esbuild": "^0.25.5",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "cheerio": "^0.22.0"
  }
}
